    <servlet-mapping>
        <servlet-name>UploadImage</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PictureBrowse</servlet-name>
        <servlet-class>PictureBrowse</servlet-class>
    </servlet>
    <servlet-mapping>
